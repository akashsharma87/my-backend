{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "vite",
    "server": "nodemon server.js",
    "build": "vite build",
    "build:analyze": "vite build && open dist/stats.html",
    "build:performance": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run src",
    "test:integration": "vitest run server",
    "test:e2e": "playwright test",
    "test:performance": "node scripts/performance-test.cjs",
    "audit:security": "node scripts/security-audit.cjs",
    "optimize:db": "node -e \"require('./server/utils/dbOptimization').createIndexes()\"",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "start:production": "NODE_ENV=production node server.js",
    "start:staging": "NODE_ENV=staging node server.js",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:production": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh production",
    "deploy:staging": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh staging",
    "docker:build": "docker build -t engineer-cv .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop engineer-cv-prod",
    "pm2:restart": "pm2 restart engineer-cv-prod",
    "pm2:logs": "pm2 logs engineer-cv-prod",
    "pm2:monit": "pm2 monit",
    "backup:db": "mongodump --db engineer-cv-production --out ./backups/db-$(date +%Y%m%d)",
    "backup:files": "tar -czf ./backups/files-$(date +%Y%m%d).tar.gz uploads/",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "logs:view": "tail -f logs/app.log",
    "logs:error": "tail -f logs/error.log",
    "maintenance:cleanup": "find ./logs -name '*.log' -mtime +30 -delete && find ./backups -name '*.tar.gz' -mtime +30 -delete"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "docx-parser": "^0.2.1",
    "dompurify": "^3.2.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "framer-motion": "^12.23.21",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "openai": "^5.20.3",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.1",
    "tesseract.js": "^6.0.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest-environment-jsdom": "^30.1.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.35",
    "rollup-plugin-visualizer": "^6.0.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
